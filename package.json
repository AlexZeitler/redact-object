{
  "name": "redact-object",
  "version": "2.0.0",
  "description": "Object redactor",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run unit",
    "lint": "semistandard --verbose | snazzy",
    "unit": "nyc jasmine-node test/unit --verbose",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunburdick/redact-object"
  },
  "bugs": {
    "url": "https://github.com/shaunburdick/redact-object/issues"
  },
  "keywords": [
    "redactor"
  ],
  "author": "Shaun Buridck <github@shaunburdick.com>",
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.11.12",
    "jasmine-node": "^1.14.5",
    "nyc": "^8.1.0",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.1"
  },
  "homepage": "https://github.com/shaunburdick/redact-object",
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "semistandard": {
    "ignore": [
      "coverage"
    ]
  },
  "nyc": {
    "include": [
      "index.js"
    ]
  }
}
